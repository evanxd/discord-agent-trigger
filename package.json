{
  "name": "discord-agent-trigger",
  "version": "1.0.0",
  "description": "A Discord bot that triggers an external agent to do tasks users request.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src __tests__ --ext .ts,.tsx",
    "lint:fix": "eslint src __tests__ --ext .ts,.tsx --fix",
    "start": "pm2 start dist/index.js --name discord-agent-trigger --node-args '--require dotenv/config'",
    "test": "jest"
  },
  "keywords": [
    "discord",
    "bot",
    "agent",
    "automation",
    "redis",
    "typescript",
    "nodejs"
  ],
  "author": "Evan Tseng-Jen Tseng <evan@tseng.io>",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "redis": "^5.8.2"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "pm2": "^6.0.13",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2"
  }
}
